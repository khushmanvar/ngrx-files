{
	"name": "ngrx-files",
	"displayName": "NGRX Files",
	"description": "Scaffold NGRX Component Store Files.",
	"version": "0.0.1",
	"repository": "https://github.com/Microsoft/vscode-extension-samples/ngrx-files",
	"publisher": "khushmanvar",
	"engines": {
		"vscode": "^1.74.0"
	},
	"icon": "./ngrx.png",
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.generateNGRXStoreFiles",
				"title": "Generate NGRX Store"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "",
					"command": "extension.generateNGRXStoreFiles",
					"group": "YourGroup@1"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"typescript": "^5.4.2"
	}
}